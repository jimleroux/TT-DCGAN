{
	"type": "ae",
	"id": "not_TTsmall",
	"training": {
		"epochs": 20,
		"batch_size": 64,
		"lr": 0.003
    },
    
	"encoder": [
        {
            "type": "conv",
            "param": {
                "in_channels": 1,
                "out_channels": 16,
                "kernel_size": 4,
                "stride": 2,
                "padding": 1}
        },
        {
            "type": "batchnorm",
            "param": {"num_features": 16}
        },
        {
            "type": "leaky_relu",
            "param": {"negative_slope": 0.2}
        },
        {
            "type": "conv",
            "param": {
                "in_channels": 16,
                "out_channels": 16,
                "kernel_size": 4,
                "stride": 2,
                "padding": 1}
        },
        {
            "type": "batchnorm",
            "param": {"num_features": 16}
        },
        {
            "type": "leaky_relu",
            "param": {"negative_slope": 0.2}
        },
        {
            "type": "conv",
            "param": {
                "in_channels": 16,
                "out_channels": 32,
                "kernel_size": 4,
                "stride": 2,
                "padding": 1}
        },
        {
            "type": "batchnorm",
            "param": {"num_features": 32}
        },
        {
            "type": "leaky_relu",
            "param": {"negative_slope": 0.2}
        },
        {
            "type": "conv",
            "param": {
                "in_channels": 32,
                "out_channels": 32,
                "kernel_size": 4,
                "stride": 2,
                "padding": 1}
        },
        {
            "type": "batchnorm",
            "param": {"num_features": 32}
        },
        {
            "type": "leaky_relu",
            "param": {"negative_slope": 0.2}
        },
        {
            "type": "conv",
            "param": {
                "in_channels": 32,
                "out_channels": 100,
                "kernel_size": 4,
                "stride": 1,
                "padding": 0}
        }
    ],

    "decoder": [
        {
            "type": "deconv",
            "param": {
                "in_channels": 100,
                "out_channels": 32,
                "kernel_size": 4,
                "stride": 1,
                "padding": 0,
                "bias": false}
        },
        {
            "type": "batchnorm",
            "param": {"num_features": 32}
        },
        {
            "type": "relu",
            "param": {}
        },
        {
            "type": "deconv",
            "param": {
                "in_channels": 32,
                "out_channels": 32,
                "kernel_size": 4,
                "stride": 2,
                "padding": 1,
                "bias": false}
        },
        {
            "type": "batchnorm",
            "param": {"num_features": 32}
        },
        {
            "type": "relu",
            "param": {}
        },
        {
            "type": "deconv",
            "param": {
                "in_channels": 32,
                "out_channels": 32,
                "kernel_size": 4,
                "stride": 2,
                "padding": 1,
                "bias": false}
        },
        {
            "type": "batchnorm",
            "param": {"num_features": 32}
        },
        {
            "type": "relu",
            "param": {}
        },
        {
            "type": "deconv",
            "param": {
                "in_channels": 32,
                "out_channels": 16,
                "kernel_size": 4,
                "stride": 2,
                "padding": 1,
                "bias": false}
        },
        {
            "type": "batchnorm",
            "param": {"num_features": 16}
        },
        {
            "type": "relu",
            "param": {}
        },
        {
            "type": "deconv",
            "param": {
                "in_channels": 16,
                "out_channels": 1,
                "kernel_size": 4,
                "stride": 2,
                "padding": 1,
                "bias": false}
        },
        {
            "type": "tanh",
            "param": {}
        }
    ]
}
